#include<iostream>
using namespace std;
class A
{
public:
int k;

void display()
{

cout<<"this display method is from base class A"<<endl;

}
};

class B:public A
{
public:

void display1()
{

cout<<"this is display method is from base class B"<<endl;

}
};
class C:public A
{
public:

void display2()
{

cout<<"this is display2 method is from base class C"<<endl;

}
};
class D:public B,public C
{
public:

void display3()
{

cout<<"this is display3 method  from base class D"<<endl;

}
